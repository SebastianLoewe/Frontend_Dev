var img;

function preload() {

  img = loadImage("https://pbs.twimg.com/profile_images/378800000532546226/dbe5f0727b69487016ffd67a6689e75a_400x400.jpeg");

}

function setup() {
  createCanvas(img.width, img.height);
}

function draw() {

  image(img, 0, 0);
  var c = img.get(mouseX, mouseX, 50, 50);
  image(c, 20, 20);

}
